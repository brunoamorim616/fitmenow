{
    "cli": {
        "version": ">= 0.45.1"
    },
    "build": {
        "development": {
            "channel": "development",
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "API_URL": "https://api.jokes.one"
            }
        },
        "preview": {
            "channel": "preview",
            "distribution": "internal",
            "env": {
                "API_URL": "https://api.production.com"
            }
        },
        "production": {
            "channel": "production",
            "env": {
                "API_URL": "https://api.production.com"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
